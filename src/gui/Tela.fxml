<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane prefHeight="560.0" prefWidth="751.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.TelaController">
   <children>
      <Pane layoutY="-9.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <ComboBox fx:id="cbOperacao" layoutX="269.0" layoutY="56.0" prefHeight="25.0" prefWidth="237.0" promptText="Selecione a operação desejada:" />
            <Button fx:id="btExecuta" layoutX="518.0" layoutY="56.0" mnemonicParsing="false" onAction="#executar" text="Executa" />
            <TextArea fx:id="taResult" editable="false" layoutX="269.0" layoutY="101.0" prefHeight="434.0" prefWidth="426.0" />
            <TreeView fx:id="tvMIB" layoutX="14.0" layoutY="58.0" prefHeight="477.0" prefWidth="200.0" />
         </children>
      </Pane>
      <MenuBar layoutX="275.0" layoutY="14.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <menus>
          <Menu mnemonicParsing="false" text="Arquivo">
            <items>
              <MenuItem fx:id="miClose" mnemonicParsing="false" onAction="#fechar" text="Close" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Ajuda">
            <items>
              <MenuItem fx:id="miSobre" mnemonicParsing="false" onAction="#sobre" text="Sobre" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </children>
</AnchorPane>
